<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name, label: "Restaurant name"%>
  <%= f.input :address, label: "The address" %>
  <%= f.input :phone_number, label: "Phone number" %>
  <%= f.select :category, ['chinese', 'italian', 'japanese', 'french', 'belgian'],label: "cuisine"%>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>

